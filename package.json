{
  "name": "myrpc",
  "productName": "MyRPC",
  "version": "1.1.2",
  "description": "Easily set your Discord status to anything!",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package --arch=ia32,x64 --platform=linux,win32",
    "make": "electron-forge make --arch=ia32,x64",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "styles": "gulp sass",
    "contrib:add": "all-contributors add",
    "contrib:generate": "all-contributors generate"
  },
  "keywords": [],
  "author": "Landon Gravat <railinator4903@gmail.com> (https://railrunner16.me)",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel",
          "appx"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm",
          "apk"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "myrpc"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "RailRunner166",
        "name": "MyRPC"
      },
      "windowsStoreConfig": {
        "packageName": "me.railrunner16.myrpc",
        "name": "MyRPC"
      }
    }
  },
  "dependencies": {
    "discord-rich-presence": "^0.0.7",
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.1.0",
    "electron-google-analytics": "^0.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "moment": "^2.23.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-moment": "^0.8.4",
    "register-scheme": "github:devsnek/node-register-scheme"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.3",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7",
    "gulp": "^4.0.0",
    "gulp-sass": "^4.0.2",
    "node-sass": "^4.11.0"
  }
}

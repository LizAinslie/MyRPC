{
  "name": "myrpc",
  "productName": "MyRPC",
  "version": "1.1.3",
  "description": "An app to easily set your Discord status to anything!",
  "main": "public/main.js",
  "dependencies": {
    "@babel/core": "7.1.6",
    "@myrpc/documents-folder": "^1.0.1",
    "@svgr/webpack": "2.4.1",
    "auto-launch": "^5.0.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-preset-react-app": "^7.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "datconfig-parser": "^1.0.2",
    "discord-rich-presence": "^0.0.7",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-google-analytics": "^0.1.0",
    "electron-prebuilt-compile": "^4.0.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.4.3",
    "moment": "^2.23.0",
    "nconf": "^0.10.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.3.1",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-app-polyfill": "^0.2.0",
    "react-dev-utils": "^7.0.1",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-moment": "^0.8.4",
    "register-scheme": "github:devsnek/node-register-scheme",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "homepage": "./",
  "scripts": {
    "react:start": "node scripts/start.js",
    "electron:start": "node ./dev-scripts/start-electron.js",
    "react:build": "node scripts/build.js",
    "react:test": "node scripts/test.js",
    
    "pack": "electron-builder --dir",
    "dist": "yarn react:build && electron-builder",
    "preelectron-pack": "yarn react:build",
    "electron-pack": "build --em.main=build/main.js",
    
    "postinstall": "electron-builder install-app-deps",
    "build": "yarn dist",
    "start": "concurrently \"yarn react:start\" \"yarn electron:start\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.4"
  },
  "build": {
    "productName": "MyRPC",
    "appId": "me.railrunner16.MyRPC",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Utility"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "MyRPC",
      "repo": "MyRPC",
      "private": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MyRPC/MyRPC.git"
  },
  "author": {
    "name": "MyRPC Maintainers",
    "email": "support@myrpc.app",
    "url": "https://myrpc.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MyRPC/MyRPC/issues"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  },
  "browserslist": "electron 1.6",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}

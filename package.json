{
  "name": "myrpc",
  "productName": "MyRPC",
  "version": "1.1.3",
  "description": "An app to easily set your Discord status to anything!",
  "dependencies": {
    "auto-launch": "^5.0.5",
    "css-loader": "^2.1.0",
    "datconfig-parser": "^1.0.2",
    "discord-rich-presence": "^0.0.7",
    "electron-builder": "^20.38.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-google-analytics": "^0.1.0",
    "electron-prebuilt-compile": "^4.0.0",
    "moment": "^2.23.0",
    "nconf": "^0.10.0",
    "node-sass": "^4.11.0",
    "platform-folders": "^0.2.7",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-moment": "^0.8.4",
    "react-scripts": "2.1.3",
    "register-scheme": "github:devsnek/node-register-scheme",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "homepage": "https://myrpc.app",
  "scripts": {
    "react:start": "react-scripts start",
    "electron:start": "node ./dev-scripts/start-electron.js",
    "start": "concurrently \"yarn react:start\" \"yarn electron:start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.0.1"
  },
  "build": {
    "productName": "MyRPC",
    "appId": "me.railrunner16.MyRPC",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Utility"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "MyRPC",
      "repo": "MyRPC",
      "private": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MyRPC/MyRPC.git"
  },
  "author": {
    "name": "MyRPC Maintainers",
    "email": "support@myrpc.app",
    "url": "https://myrpc.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MyRPC/MyRPC/issues"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  },
  "browserslist": "electron 1.6"
}
